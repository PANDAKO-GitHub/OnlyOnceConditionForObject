  "eventsFunctionsExtensions": [
    {
      "author": "",
      "description": "This extension is useful if you want to trigger only once per instance.\nThis is simpler than creating your own instance variables.\n\nAdding the \"Only Once Condition\" behavior to an object will add the \"Only Once\" condition to the instance.\nThe condition will be identified by the condition name and will be executed only once.",
      "extensionNamespace": "",
      "fullName": "Only Once Condition for Object",
      "helpPath": "https://github.com/PANDAKO-GitHub/OnlyOnceConditionForObject",
      "iconUrl": "data:image/svg+xml;base64,",
      "name": "OnlyOnceConditionForObject",
